{
    "name": "spatie/laravel-mail-preview",
    "description": "Preview mail driver for testing emails in Laravel",
    "keywords": ["laravel", "mail"],
    "homepage": "https://github.com/themsaid/laravel-preview-mail-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "themsaid@gmail.com"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/mail": "^7.0|^8.0",
        "illuminate/filesystem": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailPreview\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailPreview\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailPreview\\MailPreviewServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
